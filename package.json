{
  "name": "node-shadowsocks",
  "version": "1.0.0",
  "description": "Shadowsocks implements with nodejs",
  "main": "dist/ss-local.js",
  "scripts": {
    "ss-local": "node dist/ss-local.js",
    "ss-server": "node dist/ss-server.js",
    "build": "tsc",
    "rebuild": "rm -rf dist && npm run build",
    "dev": "nodemon --watch src -e ts --exec \"(tsc && node dist/ss-local.js) || exit 1\""
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "author": "liqiang",
  "license": "ISC",
  "dependencies": {
    "futoin-hkdf": "^1.3.3"
  }
}
